n","\n","        tokens = tokenizer.encode(label[0])\n","        tokens = torch.tensor(tokens, device=device).unsqueeze(0)\n","\n","        out = model(mel, tokens[:, :-1])\n","\n","        loss = loss_fn(\n","            out.view(-1, out.size(-1)),\n","            tokens[:, 1:].reshape(-1)\n","        )\n","\n","        optimizer.zero_grad()\n","        loss.backward()\n","        optimizer.step()\n","\n","        train_loss_epoch += loss.item()\n","        loop.set_description(f\"Epoch {epoch+1}\")\n","        loop.set_postfix(loss=loss.item())\n","        losslst.append(loss.item())\n","        print(loss.item(),\"\\n\")\n","\n","    # 검증 루프\n","    model.eval()\n","    val_losses = []\n","    with torch.no_grad():\n","        for audio, label in valid_loader:\n","            audio = audio.to(device)\n","            audio = whisper.pad_or_trim(audio)\n","            mel = whisper.log_mel_spectrogram(audio).to(device)\n","            tokens = tokenizer.encode(label[0])\n","            tokens = torch.tensor(tokens, device=device).unsqueeze(0)\n","            out = model(mel, tokens[:, :-1])\n","            val_loss = loss_fn(out.view(-1, out.size(-1)), tokens[:, 1:].reshape(-1))\n","            val_losses.append(val_loss.item())\n","\n","    val_loss_mean = sum(val_losses)/len(val_losses)\n","    print(f\"\\n✅ Epoch {epoch+1} 완료! Validation Loss: {val_loss_mean:.4f}\\n\")\n","    model.train()\n"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"hVeL0FB5jBWd","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1747909969522,"user_tz":-540,"elapsed":695,"user":{"displayName":"서희","userId":"09896160831495652044"}},"outputId":"3e6f362b-3026-46b8-ec99-be6a378dc0f1"},"outputs":[{"output_type":"stream","name":"stdout","text":["\n","📦 모델이 저장되었습니다: /content/drive/MyDrive/인지응팀플/0522_validation.pt\n"]}],"source":["\n","# 8. 모델 저장\n","save_path = \"/content/drive/MyDrive/인지응팀플/0522_validation.pt\"\n","os.mak